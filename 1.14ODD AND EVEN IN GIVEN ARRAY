//Print odd and even in given array
#include<stdio.h>

int main(){
	int n,i;
	printf("Enter the size of array :");
	scanf("%d",&n);  
	int arr[n];
	printf("\nEnter elements of array :");
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	printf("\nOdd numbers in array: ");
	for(i=0;i<n;i++){
		if(arr[i]%2!=0)
		printf("%d ",arr[i]);
	}
	printf("\nEven numbers in array: ");
	for(i=0;i<n;i++){
		if(arr[i]%2==0)
		printf("%d ",arr[i]);
	}
	return 0;
}
