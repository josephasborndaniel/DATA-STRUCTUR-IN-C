#include<stdio.h>
int main()
{
	int num,factorial=1,i=1;
	
	printf("enter a number");
	scanf("%d",&num);
	
	while(i<=num)
	{
		factorial=factorial*i;
		i++;
	}
	printf("%d",factorial);
	return 0;
}

